{
  "contract_application": {
    "type": "object",
    "properties": {
      "clinic_location_id": {
        "type": "integer"
      },
      "plan_code": {
        "type": "integer"
      },
      "owner_first_name": {
        "type": "string",
        "maxLength": 20
      },
      "owner_last_name": {
        "type": "string",
        "maxLength": 30
      },
      "address": {
        "type": "string",
        "maxLength": 30
      },
      "city": {
        "type": "string",
        "maxLength": 19
      },
      "state": {
        "type": "string",
        "maxLength": 2
      },
      "zip": {
        "type": "string",
        "maxLength": 10
      },
      "country": {
        "type": "string",
        "maxLength": 20
      },
      "mobile": {
        "type": "string",
        "maxLength": 20
      },
      "phone": {
        "type": "string",
        "maxLength": 20
      },
      "email": {
        "type": "string",
        "maxLength": 60
      },
      "owner_id": {
        "type": "string",
        "maxLength": 40
      },
      "pet_id": {
        "type": "string",
        "maxLength": 40
      },
      "pet_name": {
        "type": "string"
      },
      "age": {
        "type": "string"
      },
      "gender": {
        "type": "string"
      },
      "payment_name": {
        "type": "string"
      },
      "expiration_month": {
        "type": "integer",
        "minimum": 1,
        "maximum": 12
      },
      "expiration_year": {
        "type": "integer",
        "minimum": 2000,
        "maximum": 2999
      },
      "card_number": {
        "type": "integer"
      },
      "initial_payment_option": {
        "type": "integer",
        "enum": [
          1,
          12
        ]
      },
      "optional_plan_services": {
        "type": "array"
      }
    },
    "required": []
  },
  "contract_applications_create": {
    "$ref": "#contract_application",
    "required": [
      "clinic_location_id",
      "plan_code",
      "owner_first_name",
      "owner_last_name",
      "address",
      "city",
      "state",
      "zip",
      "country",
      "mobile",
      "phone",
      "email",
      "owner_id",
      "pet_id",
      "pet_name",
      "age",
      "gender",
      "card_name",
      "payment_name",
      "expiration_month",
      "expiration_year",
      "card_number",
      "initial_payment_option",
      "optional_plan_services"
    ]
  },
  "contract_applications_update": {
    "type": "object",
    "properties": {
      "allOf": [
        {
          "$ref": "#contract_application"
        },
        {
          "required": []
        }
      ],
      "phone1": {
        "minLength": 1
      },
      "firstName": {
        "minLength": 1
      },
      "lastName": {
        "minLength": 1
      },
      "address1": {
        "minLength": 1
      },
      "city": {
        "minLength": 1
      },
      "state": {
        "minLength": 1
      },
      "postalCode": {
        "minLength": 1
      },
      "payMethod": {
        "minLength": 1
      },
      "accountNbrForDisplay": {
        "minLength": 1
      },
      "expirationMonth": {
        "minLength": 1
      },
      "expirationYear": {
        "minLength": 1
      },
      "paymentName": {
        "minLength": 1
      }
    }
  }
}