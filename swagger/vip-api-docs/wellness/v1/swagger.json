  "paths": {
    "/api/v1/wellness/contract_applications/agreements/{id}": {
      "get": {
        "summary": "Generate a new agreement document to complete contract application. Requires contract application ID.",
        "tags": [
          "Agreements"
        ],
        "security": [
          {
            "bearer_auth": [

            ]
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Generate a new agreement PDF"
          },
          "404": {
            "description": "Document not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/error"
                }
              }
            }
          },
          "403": {
            "description": "Invalid credentials",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/auth_error"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Upload a signed agreement document",
        "tags": [
          "Agreements"
        ],
        "security": [
          {
            "bearer_auth": [

            ]
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/agreement"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Upload signed PDF document"
          },
          "404": {
            "description": "Document not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/error"
                }
              }
            }
          },
          "403": {
            "description": "Invalid credentials",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/auth_error"
                }
              }
            }
          }
        }
      }
    },
